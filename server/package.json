{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "bin": {
    "console": "bin/console"
  },
  "scripts": {
    "start": "babel-node ./bin/www",
    "start_NODEMON": "nodemon --exec babel-node ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start_old": "node ./bin/www",
    "build___": "mkdir dist && babel src -s -d dist",
